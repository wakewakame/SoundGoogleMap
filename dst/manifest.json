{
	"manifest_version": 2,
	"name": "GoogleMapChanger",
	"description": "you can change the shader of the Google Map with this tool.",
	"version": "1",
	"icons": {
		"16": "icon/16.png",
		"48": "icon/48.png",
		"128": "icon/128.png"
	},

	"content_scripts": [
		{
			"matches": ["https://www.google.com/maps/*"],
			"run_at": "document_end",
			"js": ["content-script.js"]
		}
	],

	"web_accessible_resources": [
		"main.js"
	],
	"permissions": [
		"<all_urls>"
	]
}